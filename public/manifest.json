{
  "manifest_version": 3,
  "name": "Puuuush - Tab Manager",
  "version": "1.0.0",
  "description": "A Toby-like tab manager for Chrome.",
  "permissions": [
    "tabs",
    "storage",
    "favicon",
    "contextMenus"
  ],
  "action": {},
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "vite.svg",
    "48": "vite.svg",
    "128": "vite.svg"
  }
}
